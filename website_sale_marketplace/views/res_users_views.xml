<odoo>
    <record id="view_users_form_inherit_marketplace" model="ir.ui.view">
        <field name="name">res.users.form.inherit.marketplace</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[hasclass('oe_title')]" position="inside">
                <group>
                    <field name="is_marketplace_vendor"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>